# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key

# Next.js Public Variables (exposed to browser)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google OAuth (Optional - if using Google authentication)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Application Configuration
NODE_ENV=development
PORT=8080

# CORS Configuration (Production only - comma-separated list of allowed origins)
# Example: ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com
# Leave as * for development
ALLOWED_ORIGINS=*

# Google Cloud Configuration (for deployment)
# GCP_PROJECT_ID=your-gcp-project-id
# GCP_REGION=us-central1
